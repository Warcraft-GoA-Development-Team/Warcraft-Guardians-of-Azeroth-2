## Lich Variant 1

###lich1gold
pdxmesh = {
	name = "lich1_mesh"
	file = "lich1.mesh"

	meshsettings = {
		name = "lich1Shape"
		index = 0
		texture_diffuse = "lichgold.dds"
		texture_normal = "default_yellow_normal.dds"
		texture_specular = "male_elven_eyebrows_0_properties.dds"
		shader = "portrait_attachment"
		shader_file = "gfx/FX/jomini/portrait.shader"
	}
}
entity = {
	name = "lich1gold_entity"
	pdxmesh = "lich1_mesh"
}

